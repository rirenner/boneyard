<config>

    <native xmlns="http://cisco.com/ns/yang/ned/ios">
      <ip>
        <access-list>
          <extended>
            <name>TestACL</name>
            <access-list-seq-rule>
              <sequence>10</sequence>
              <permit>
                <acl-protocol>
                  <tcp>
                    <source-destination-match>
                      <host>10.0.0.2</host>
                      <dst-host>10.0.0.3</dst-host>
                      <acl-match-attribute>
                        <tcp-match>
                          <eq>
                            <port-operand>22</port-operand>
                          </eq>
                        </tcp-match>
                      </acl-match-attribute>
                    </source-destination-match>
                  </tcp>
                </acl-protocol>
              </permit>
            </access-list-seq-rule>
            <access-list-seq-rule>
              <sequence>20</sequence>
              <permit>
                <acl-protocol>
                  <tcp>
                    <source-destination-match>
                      <host>10.0.0.3</host>
                      <src-tcp-eq>22</src-tcp-eq>
                      <dst-host>10.0.0.2</dst-host>
                    </source-destination-match>
                  </tcp>
                </acl-protocol>
              </permit>
            </access-list-seq-rule>
            <access-list-seq-rule>
              <sequence>30</sequence>
              <deny>
                <acl-protocol>
                  <ip>
                    <source-destination-match>
                      <host>10.0.0.2</host>
                      <dst-host>10.0.0.3</dst-host>
                    </source-destination-match>
                  </ip>
                </acl-protocol>
              </deny>
            </access-list-seq-rule>
            <access-list-seq-rule>
              <sequence>40</sequence>
              <deny>
                <acl-protocol>
                  <ip>
                    <source-destination-match>
                      <host>10.0.0.3</host>
                      <dst-host>10.0.0.2</dst-host>
                    </source-destination-match>
                  </ip>
                </acl-protocol>
              </deny>
            </access-list-seq-rule>
            <access-list-seq-rule>
              <sequence>50</sequence>
              <permit>
                <acl-protocol>
                  <ip>
                    <source-destination-match>
                      <any/>
                      <dst-any/>
                    </source-destination-match>
                  </ip>
                </acl-protocol>
              </permit>
            </access-list-seq-rule>
          </extended>
        </access-list>
      </ip>
      <vlan>
        <access-map>
          <word>TestVACL</word>
          <action>forward</action>
          <match>
            <ipv6>
              <address>TestACL</address>
            </ipv6>
          </match>
        </access-map>
        <filter>
          <word>TestVACL</word>
          <vlan-list>10</vlan-list>
        </filter>
      </vlan>
    </native>
</config>
